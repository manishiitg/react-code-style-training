{
  "name": "react-code-style-training",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@storybook/react": "^5.3.19",
    "bootstrap": "^5.0.0-alpha1",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "node-request-interceptor": "^0.2.6",
    "node-sass": "^4.14.1",
    "popper.js": "^1.16.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.1.1"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.1",
    "@storybook/react": "^5.3.19"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.1.3",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.3",
    "@testing-library/user-event": "^12.0.11",
    "msw": "^0.19.5",
    "nock": "^13.0.2"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@1.0.19",
      "tester": "bit.envs/testers/jest@22.4.3"
    },
    "componentsDefaultDirectory": "src/components/{name}",
    "packageManager": "yarn",
    "overrides": {
      "*css*": {
        "env": {
          "tester": "-"
        }
      }
    }
  }
}
